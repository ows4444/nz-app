{
  "singleQuote": true,
  "semi": true,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": ["^@nestjs/(.*)$", "", "^@nz/(.*)$", "", "^@/(.*)$", "^[a-z]", "", "^[./]", ""],
  "importOrderParserPlugins": ["typescript", "decorators-legacy", "classProperties"],
  "trailingComma": "all",
  "printWidth": 200,
  "endOfLine": "auto"
}
